{
  "name": "react",
  "version": "1.0.0",
  "description": "A repository tracker to get an overview of your work without visiting the site itself.",
  "main": "index.js",
  "scripts": {
    "test": "jest --watch --verbose --setupFilesAfterEnv ./src/test/setupTests.js",
    "coverage": "jest --setupFilesAfterEnv ./src/test/setupTests.js --coverage --silent --watchAll=false",
    "dev": "webpack-cli serve --mode development --config config/webpack.config.dev.js",
    "build": "webpack --config config/webpack.config.production.js"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yusra-tahir/Repository-Tracker"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^13.1.1",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.1",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^5.0.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.2",
    "jest": "^26.6.3",
    "style-loader": "^2.0.0",
    "webpack": "^5.6.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "mdb-react-ui-kit": "^3.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^8.0.1",
    "react-router-dom": "^6.3.0",
    "redux": "^4.2.0",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.4.1"
  },
  "bugs": {
    "url": "https://github.com/yusra-tahir/Repository-Tracker/issues"
  },
  "homepage": "https://github.com/yusra-tahir/Repository-Tracker#readme"
}
